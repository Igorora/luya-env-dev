{
    "name" : "luyadev/luya-env-dev",
    "description" : "LUYA Dev Env",
    "require" : {
        "luyadev/luya" : "~1.0.1",
        "luyadev/luya-module-admin" : "~1.0.0",
        "luyadev/luya-module-cms" : "~1.0.0",
        "cpliakas/git-wrapper": "~1.0"
    },
    "require-dev" : {
        "yiisoft/yii2-debug" : "~2.0.0",
        "yiisoft/yii2-gii" : "~2.0.0"
    },
    "autoload" : {
        "psr-4" : {
            "luya\\" : "repos/luya/core",
            "luya\\admin\\" : "repos/luya-module-admin/src",
            "luya\\cms\\" : "repos/luya-module-cms/src",
            "luya\\news\\" : "repos/luya-module-news/src",
            "luya\\contactform\\" : "repos/luya-module-contactform/src"
        }
    },
    "config": {
        "fxp-asset" : {
            "pattern-skip-version": "(-build|-patch)",
             "installer-paths": {
                "bower-asset-library": "vendor/bower"
            }
        }   
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "cachestorage.json" : "0777"
                }
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
